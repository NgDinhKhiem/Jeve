// Simple arithmetic
a = 10;
b = 5;
sum = a + b;
print("Sum: " + sum);

// Conditional statement
if (sum > 10) {
    print("Sum is greater than 10");
} else {
    print("Sum is less than or equal to 10");
}

// Loop
i = 0;
while (i < 5) {
    print("Count: " + i);
    i = i + 1;
} 