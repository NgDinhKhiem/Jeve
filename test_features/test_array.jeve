// Simple array test
print("Starting test");

// Create array
arr = [1, 2, 3];
print("Array created: " + arr);

// Print array
print("Array: " + arr);

// Modify array
arr[1] = 42;
print("Modified array: " + arr);

// Test array assignment and for loop
arr = [1, 2, 3, 4];
arr[0] = 5;

for i = 0 to 3 {
    print("Iteration " + i + ": array: " + arr[i]);
}

// Test insert
insert(arr, 2, 99);
print("After insert at index 2: " + arr);

// Test delete
delete(arr, 1);
print("After delete at index 1: " + arr);

